<h1 class="mt-3">Resultado  Division de Numeros</h1>

{{#if array}}
<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Numero 1</th>
      <th scope="col">Numero 2</th>
    </tr>
  </thead>
  <tbody>
    {{#each array}}
    <tr>
      <th scope="row">{{numeromayor}}</th>
      <th scope="row">{{numeromenor}}</th>
    </tr>
    {{/each}}
  </tbody>
</table>
{{/if}}
